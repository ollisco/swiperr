{
    "presets": [
        "module:metro-react-native-babel-preset",
        "babel-preset-expo"

    ],
    "plugins": [
        ["module:react-native-dotenv", {
        "envName": "APP_ENV",
        "moduleName": "@env",
        "path": ".env",
        "blacklist": null,
        "whitelist": null,
        "safe": false,
        "allowUndefined": true
        }]
        ]
}